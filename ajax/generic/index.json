  "id": "com.sap.sac.sample.ajax.generic",
  "version": "1.0.0",
  "name": "Ajax Generic Widget",
  "newInstancePrefix": "AjaxGeneric",
  "description": "A sample custom widget wrapped jQuery ajax generic get/post",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-sample-ajax-generic",
      "url": "http://localhost:3000/ajax/generic/main.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
		"width": {
			"type": "integer",
			"default": 1
		},
		"height": {
			"type": "integer",
			"default": 1
		}
  },
  "methods": {
		"get": {
			"description": "Wrapper for jQuery get",
			"parameters": [
				{
					"name": "url",
					"type": "string",
					"description": ""
				},
				{
					"name": "data",
					"type": "any",
					"description": ""
				},
				{
					"name": "dataType",
					"type": "string",
					"description": ""
				}
			],
			"returnType": "string"
		},
		"post": {
			"description": "Wrapper for jQuery post",
			"parameters": [
				{
					"name": "url",
					"type": "string",
					"description": ""
				},
				{
					"name": "data",
					"type": "any",
					"description": ""
				},
				{
					"name": "dataType",
					"type": "string",
					"description": ""
				}
			],
			"returnType": "any"
		}
  },
  "events": {

  }
